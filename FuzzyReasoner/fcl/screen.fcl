    RULE 2 : IF screensize IS small AND resolution IS normal 
                THEN video IS low;

    RULE 3 : IF screensize IS normal AND resolution IS small 
                THEN video IS low;
              
                
    // AVERAGE: both normal, one big and the other small
    
    RULE 4 : IF screensize IS normal AND resolution IS normal
    			THEN video IS average;
    			
