    RULE 7 : IF screensize IS big AND resolution IS normal 
                THEN video IS high;
                
    RULE 8 : IF screensize IS normal AND resolution IS big 
                THEN video IS high;
            
                
    // VERY HIGH: both big 
    RULE 9 : IF screensize IS big AND resolution IS big 
                THEN video IS very_high;


END_RULEBLOCK

